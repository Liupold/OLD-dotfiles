! Font
st.font: Fira Code-12;
URxvt.font: xft:Fira Code:style=Medium:size=12:antialias=true
UXTerm*faceSize:10

! UI
URxvt.scrollBar: false
URxvt.depth: 32

st.alpha: 0.85
! URxvt.tintColor: #20201d

! Extensions
URxvt.perl-ext: default,url-select,clipboard
URxvt.keysym.Meta-c: perl:clipboard:copy
URxvt.keysym.Meta-v: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common:  resize-font
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.underline: true

! Utils

URxvt.urlLauncher: chromium
URxvt.underlineURLs: true


! Colors

! special
*.foreground:   #a6a28c
*.cursorColor:  #a6a28c
*.background:   #000000

! black
*.color0:       #20201d
*.color8:       #7d7a68

! red
*.color1:       #d73737
*.color9:       #d73737

! green
*.color2:       #60ac39
*.color10:      #60ac39

! yellow
*.color3:       #cfb017
*.color11:      #cfb017

! blue
*.color4:       #6684e1
*.color12:      #6684e1

! magenta
*.color5:       #b854d4
*.color13:      #b854d4

! cyan
*.color6:       #1fad83
*.color14:      #1fad83

! white
*.color7:       #a6a28c
*.color15:      #fefbec


st.termname: st-256color
